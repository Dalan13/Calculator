<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#040404"
    android:layout_marginTop="0.5dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:orientation="vertical"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView_history_2"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="0dp"
                android:textColor="@android:color/black"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/textView_history"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:textColor="@color/black"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/textView_input_numbers"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="5dp"
                android:textColor="@color/white"
                android:textSize="24sp" />
        </LinearLayout>
    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#B1ADAD"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="8"
            android:layout_marginTop="0.1dp"
            android:layout_marginBottom="0.1dp"
            android:background=""
            android:orientation="vertical">
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginTop="0.1dp"
            android:background="#B1ADAD"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#B1ADAD"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button_clear"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="C"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="18sp"
                    app:backgroundTint="#070707" />

                <Button
                    android:id="@+id/button_clear_last"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="@string/clear_last"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="18sp"
                    app:backgroundTint="#070707" />

                <Button
                    android:id="@+id/button_parentheses"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="( )"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_pow"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="^"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="24sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_division"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="\u00F7"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="24sp"
                    app:backgroundTint="#040404" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#B1ADAD"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button_sin"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="sin"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_seven"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="7"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_eight"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background="\"
                    android:text="8"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_nine"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="9"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_multiplication"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="x"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#B1ADAD"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button_cos"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="cos"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_four"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="4"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_five"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="5"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_six"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="6"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_subtraction"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="\u2014"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="16sp"
                    app:backgroundTint="#040404" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#B1ADAD"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button_tg"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="tan"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_one"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="1"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_two"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="2"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_three"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="3"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_addition"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="+"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="24sp"
                    app:backgroundTint="#040404" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="#B1ADAD"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/button_ctg"
                    android:layout_width="89dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_marginBottom="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="ctg"
                    android:textColor="@android:color/holo_red_light"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_pi"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_marginBottom="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="&#960;"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_zero"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_marginBottom="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="0"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_dot"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_marginBottom="0.1dp"
                    android:layout_weight="1"
                    android:background=""
                    android:text="."
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    app:backgroundTint="#040404" />

                <Button
                    android:id="@+id/button_equal"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="0.1dp"
                    android:layout_marginTop="0.1dp"
                    android:layout_marginRight="0.1dp"
                    android:layout_marginBottom="0.1dp"
                    android:layout_weight="1"
                    android:background="@android:color/holo_red_dark"
                    android:text="="
                    android:textColor="@android:color/white"
                    android:textSize="24sp"
                    app:backgroundTint="@android:color/holo_red_dark" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="8"
            android:layout_marginTop="0.1dp"
            android:layout_marginBottom="0.1dp"
            android:background=""
            android:orientation="vertical">
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
